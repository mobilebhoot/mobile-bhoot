{
  "name": "pocketshield-io",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "lint": "eslint .",
    "build:android": "expo build:android",
    "build:ios": "expo build:ios",
    "eject": "expo eject",
    "build:apk:debug": "cd android && ./gradlew assembleDebug",
    "build:apk:release": "cd android && ./gradlew assembleRelease",
    "build:apk:debug:install": "cd android && ./gradlew assembleDebug && adb install -r app/build/outputs/apk/debug/app-debug.apk",
    "build:apk:release:install": "cd android && ./gradlew assembleRelease && adb install -r app/build/outputs/apk/release/app-release.apk",
    "build:ipa:debug": "cd ios && xcodebuild -workspace PocketShieldio.xcworkspace -scheme PocketShieldio -configuration Debug -destination generic/platform=iOS -archivePath build/PocketShieldio.xcarchive archive",
    "build:ipa:release": "cd ios && xcodebuild -workspace PocketShieldio.xcworkspace -scheme PocketShieldio -configuration Release -destination generic/platform=iOS -archivePath build/PocketShieldio.xcarchive archive",
    "build:ipa:export": "cd ios && xcodebuild -exportArchive -archivePath build/PocketShieldio.xcarchive -exportPath build/ipa -exportOptionsPlist ExportOptions.plist",
    "clean:android": "cd android && ./gradlew clean",
    "clean:ios": "cd ios && xcodebuild clean -workspace PocketShieldio.xcworkspace -scheme PocketShieldio",
    "clean:all": "npm run clean:android && npm run clean:ios && rm -rf node_modules && npm install",
    "dev:android": "npm run build:apk:debug:install && adb logcat | grep -E '(PocketShield|ReactNative|Expo)'",
    "dev:ios": "npm run build:ipa:debug && xcrun simctl install booted ios/build/PocketShieldio.app",
    "debug:check": "node debug-device.js check",
    "debug:logs": "node debug-device.js logs",
    "debug:install": "node debug-device.js install",
    "debug:permissions": "node debug-device.js permissions",
    "debug:clear": "node debug-device.js clear",
    "debug:info": "node debug-device.js info"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/netinfo": "11.3.1",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/stack": "^6.3.29",
    "expo": "~51.0.0",
    "expo-application": "~5.9.1",
    "expo-background-fetch": "~12.0.0",
    "expo-build-properties": "~0.12.5",
    "expo-constants": "~16.0.2",
    "expo-contacts": "~13.0.5",
    "expo-crypto": "~13.0.0",
    "expo-dev-client": "~4.0.29",
    "expo-device": "~6.0.2",
    "expo-file-system": "~17.0.0",
    "expo-font": "~12.0.10",
    "expo-linear-gradient": "~13.0.0",
    "expo-location": "~17.0.1",
    "expo-media-library": "~16.0.5",
    "expo-network": "~6.0.1",
    "expo-notifications": "~0.28.0",
    "expo-secure-store": "~13.0.0",
    "expo-sms": "~12.0.1",
    "expo-splash-screen": "~0.27.7",
    "expo-status-bar": "~1.12.0",
    "expo-task-manager": "~11.8.0",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-animatable": "^1.4.0",
    "react-native-chart-kit": "^6.12.0",
    "react-native-device-info": "^10.12.0",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-permissions": "^4.1.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-toast-message": "^2.2.0",
    "react-native-vector-icons": "^10.1.0",
    "react-native-webview": "13.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.14",
    "eslint": "^8.45.0",
    "jest": "^29.2.1",
    "sharp": "^0.34.4"
  },
  "private": true,
  "description": "Advanced Android security monitoring and threat detection app",
  "keywords": [
    "security",
    "android",
    "threat-detection",
    "vulnerability-scanning",
    "network-monitoring",
    "pocketshield"
  ],
  "author": "PocketShield.io Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pocketshield/pocketshield-io.git"
  },
  "homepage": "https://pocketshield.io"
}
